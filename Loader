local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("TRXDENT")

local serv = win:Server("TRXDENT Loader", "")

local mm2 = serv:Channel("Murder Mystery 2")
local roghoul = serv:Channel("Ro-Ghoul")
local DragonBallFinalRemastered = serv:Channel("DBFR")
local other = serv:Channel("Other Scripts")

-- Murder Mystery 2
mm2:Button("TRXDENT MM2 Status", function()
	DiscordLib:Notification("Notification", "made by Dxniz#5321", "Thank You!")
	loadstring(game:HttpGetAsync(("https://raw.githubusercontent.com/DxnizA/XDENTLoader/main/MM2Status")))()
end)

mm2:Seperator()

mm2:Button("TRXDENT MM2 Autofarm", function()
	DiscordLib:Notification("Notification", "made by Dxniz#5321", "Thank You!")
	loadstring(game:HttpGetAsync(("https://raw.githubusercontent.com/DxnizA/TRXDENT/main/MM2")))()
end)

