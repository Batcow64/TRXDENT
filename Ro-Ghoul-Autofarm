local function useSKL()
    spawn(function()
        while wait(0.3) do
            local remt =game.Players.LocalPlayer.Character:FindFirstChild("Remotes")
            repeat
                wait(0.3)
            until remt
            local keyev = remt:FindFirstChild("KeyEvent")
            repeat
                wait(0.2)
            until keyev
            if sett1ngs.autoe == true then
                local A_1 = "操你💦💔🍑👌💦操你💦💔🍑👌💦💔🍑👌💦💔🍑👌💔🍑👌💦💔🍑👌"
                local A_2 = "E"
                local A_3 = "Down"
                local Event = game:GetService("Workspace").FearlessBreaker.Remotes.KeyEvent
                Event:FireServer(A_1, A_2, A_3)
                wait(0.05)
            end
            if sett1ngs.autor == true then
                local A_1 = "操你💦💔🍑👌💦操你💦💔🍑👌💦💔🍑👌💦💔🍑👌💔🍑👌💦💔🍑👌"
                local A_2 = "R"
                local A_3 = "Down"
                local Event = game:GetService("Workspace").FearlessBreaker.Remotes.KeyEvent
                Event:FireServer(A_1, A_2, A_3)
                wait(0.05)
            end
            if sett1ngs.autof == true then
                local A_1 = "操你💦💔🍑👌💦操你💦💔🍑👌💦💔🍑👌💦💔🍑👌💔🍑👌💦💔🍑👌"
                local A_2 = "F"
                local A_3 = "Down"
                local Event = game:GetService("Workspace").FearlessBreaker.Remotes.KeyEvent
                Event:FireServer(A_1, A_2, A_3)
                wait(0.05)
            end
            if sett1ngs.autoc == true then
                local A_1 = "操你💦💔🍑👌💦操你💦💔🍑👌💦💔🍑👌💦💔🍑👌💔🍑👌💦💔🍑👌"
                local A_2 = "C"
                local A_3 = "Down"
                local Event = game:GetService("Workspace").FearlessBreaker.Remotes.KeyEvent
                Event:FireServer(A_1, A_2, A_3)
                wait(0.05)
            end
        end
    end)
end
useSKL()

local function wbSend()
    local url = Webhook.weblink
    local data = {
		["type"] = "TEST",
		["content"] = "TEST",
		["tts"] = false,
		["embed"] = {
			["title"] = "Hello",
			["description"] = "Hi"
		}
	}
	local newdata = HttpServ:JSONEncode(data)
	
	HttpServ:PostAsync(url, newdata)
end

