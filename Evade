getgenv().Settings = {
    EvadeAutoFarm = false;
    EvadeSoundID = "6081484531";
    EvadeImageID = "10526778757";
}

local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()
local win = DiscordLib:Window("TRXDENT")
local serv = win:Server("TRXDENT Evade", "")
local main = serv:Channel("Main")
local npcchanger = serv:Channel("NPC Changer")
local path = game:GetService("Workspace").Game.Players
local player = game.Players.LocalPlayer
local char = player.Character
local part = game.Workspace:FindFirstChild("Gay")
local rnwerqwr = math.random(10000,50000)

if not part then
    local prt = Instance.new("Part")
    prt.Name = "Gay"
    prt.Parent = game.Workspace
    prt.Anchored = true
    prt.CFrame = CFrame.new(-5.972589015960693, rnwerqwr, -31.89034080505371)
    prt.Transparency = 1
    prt.BrickColor = BrickColor.new(22,22,22)
else
    print("Have Part")
end
main:Toggle("Auto-Farm (Win/Money)", Settings.EvadeAutoFarm, function(t)
    if t == true then
        Settings.EvadeAutoFarm = true
        DiscordLib:Notification("TRXDENT", "Auto-Farm (Win/Money) Enabled!", "Okay!")
    else
        Settings.EvadeAutoFarm = false
        DiscordLib:Notification("TRXDENT", "Auto-Farm (Win/Money) Disabled!", "Okay!")
    end
end)

function asdtyrey()
    if Settings.EvadeAutoFarm then
        local hroot = char:FindFirstChild("HumanoidRootPart")
        if hroot then
            hroot.CFrame = CFrame.new(game.Workspace.Gay.Position.X, game.Workspace.Gay.Position.Y+6, game.Workspace.Gay.Position.Z)
        end
    end
end

function smaahh()
    for playeri, playerv in pairs(game.Players:GetChildren()) do
        for npci, npcv in pairs(path:GetChildren()) do
            if npcv.Name ~= playerv.Name then
                local HRP = npcv:FindFirstChild("HRP")
                if HRP then
                    local sound = HRP.Idle
                    local image = HRP.HeadPos.BillboardGui.ImageLabel
                    if sound.SoundId ~= "rbxassetid://"..Settings.EvadeSoundID then
                        sound.SoundId = "rbxassetid://"..Settings.EvadeSoundID
                    end
                    if image.Image ~= "rbxassetid://"..Settings.EvadeImageID then
                        image.Image = "rbxassetid://"..Settings.EvadeImageID
                    end
                end
            end
        end
    end
end

npcchanger:Button("Apply Settings (Doesn't Works on some npc's)", function()
    smaahh()
    DiscordLib:Notification("TRXDENT", "Applied Settings!", "Okay!")
end)

npcchanger:Seperator()

npcchanger:Textbox("Sound ID", "Type here!", true, function(t)
    Settings.EvadeSoundID = t
end)

npcchanger:Textbox("Image ID", "Type here!", true, function(t)
    Settings.EvadeImageID = t
end)

while true and wait() do
    asdtyrey()
end
