end)

aut4:Button("Boa Hancock's Island", function()
    root.CFrame = CFrame.new(11062.6533, 954.30835, 15120.6396)
    DiscordLib:Notification("Notification", "Succesfuly Teleported!", "Okay!")
end)

-- Settings

aut3:Button("God Mode (For Boss/NPC Farm)", function()
    game:GetService("Players").LocalPlayer.Character.Values.Block:Destroy()
    DiscordLib:Notification("Notification", "God Mode Enabled!", "Okay!")
end)

