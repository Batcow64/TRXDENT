    end
end)

aut4:Slider("Cooldown", 0, 240, STa.safecooldown, function(t)
    STa.safecooldown = t
end)

aut4:Seperator()

aut4:Button("Safezone", function()
    root.CFrame = CFrame.new(686.581298828125, 1164.6793212890625, -495.5419921875)
    DiscordLib:Notification("Notification", "Succesfuly Teleported!", "Okay!")
end)

